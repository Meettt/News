<!-- Like Button Ajax -->
<script>
function likeButtonCall(addNewsId,userId) {
	var likebutton = document.getElementById("likebutton["+addNewsId+"]");
	if(likebutton.innerHTML=='Like'){
	var ajaxRequest = new XMLHttpRequest();
	ajaxRequest.onreadystatechange = function(){
		if(ajaxRequest.readyState==4){
			var likeValue = document.getElementById("likeValue["+addNewsId+"]");
			likeValue.innerHTML=ajaxRequest.responseText;
			likebutton.innerHTML="DisLike";	
		}
	}
	ajaxRequest.open("GET","${pageContext.request.contextPath}/AjaxScript?string=like&addNewsId="+addNewsId+"&userId="+userId,true);
	ajaxRequest.send();
	}
	if(likebutton.innerHTML=="DisLike"){
		var ajaxRequest = new XMLHttpRequest();
		ajaxRequest.onreadystatechange = function(){
			if(ajaxRequest.readyState==4){
				var likeValue = document.getElementById("likeValue["+addNewsId+"]");
				likeValue.innerHTML=ajaxRequest.responseText;
				likebutton.innerHTML="Like";	
			}
		}
		ajaxRequest.open("GET","${pageContext.request.contextPath}/AjaxScript?string=Dislike&addNewsId="+addNewsId+"&userId="+userId,true);
		ajaxRequest.send();
		
	}
	
}
function likeall(addNewsId,userId) {
	var likebutton = document.getElementById("likebutton["+addNewsId+"]");
	var ajaxRequest = new XMLHttpRequest();
	ajaxRequest.onreadystatechange = function(){
		if(ajaxRequest.readyState==4){
			var likeValue = document.getElementById("likeValue["+addNewsId+"]");
			var string =ajaxRequest.responseText.split(" ", 10);
			if(string[0]==null){string[0]="Like";}
			likebutton.innerHTML=string[0];
			likeValue.innerHTML=string[1];
		}
	}
	ajaxRequest.open("GET","${pageContext.request.contextPath}/AjaxScript?string=likeAll&addNewsId="+addNewsId+"&userId="+userId,true);
	ajaxRequest.send();

	
}

function searchUserForLike(addNewsId) {
	var ajaxRequest = new XMLHttpRequest();
	ajaxRequest.onreadystatechange = function(){
		if(ajaxRequest.readyState==4){
			var obj= JSON.parse(ajaxRequest.responseText);
			for( i =0 ;i<obj.length;i++){
				debugger;
				var LikeUser = document.createElement("div");
				var LikeUserClass = document.createAttribute("class");
				LikeUserClass.value="Like-User";
				
				LikeUser.setAttributeNode(LikeUserClass);
				
				var a= document.createElement("a");
				LikeUser.appendChild(a);
				var img=document.createElement("img");
				var imgSrc=document.createAttribute("src");
				imgSrc.value="img/18-80x80.jpg";
				img.setAttributeNode(imgSrc);
				var imgClass=document.createAttribute("class");
				imgClass.value="attachment-80x80 wp-post-image";
				img.setAttributeNode(imgClass);
				
				LikeUser.appendChild(img);
				var Likename=document.createElement("h3");
				var LikenameClass=document.createAttribute("class")
				LikenameClass.value="border_none";
				var LikenameId=document.createAttribute("id")
				LikenameId.value="Like-name";
				Likename.setAttributeNode(LikenameId);
				Likename.setAttributeNode(LikenameClass);
				var userName=document.createElement("a");
				var userNameId=document.createAttribute("id")
				userNameId.value="userName";
				userName.setAttributeNode(userNameId);
				userName.innerHTML=obj[i].name;
				
				Likename.appendChild(userName);
				LikeUser.appendChild(Likename);
				document.getElementById("likeDiv").appendChild(LikeUser);
				
				
			}
						
		}
		return document.getElementById("likeDiv");
	}
	ajaxRequest.open("GET","${pageContext.request.contextPath}/AjaxScript?string=userLike&addNewsId="+addNewsId,true);
	ajaxRequest.send();

	
}

